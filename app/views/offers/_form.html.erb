<div class="container">
  <h4>Estas ofertando para el Usario: </h4>
  <%= simple_form_for @offer do |f| %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :date_from, html5:true %>
    <%= f.input :date_to, html5:true %>
    <%= f.input :photo, as: :file %>
    <%= f.submit %>
  <% end %>
</div>
