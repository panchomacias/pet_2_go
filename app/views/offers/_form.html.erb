<div class="container">
  <%= simple_form_for ([@user, @offer]) do |f| %>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :price %>
    <%= f.input :date_from, html5:true %>
    <%= f.input :date_to, html5:true %>
    <%= f.input :photo, as: :file %>

    <%= f.submit %>
  <% end %>
</div>
